{
    "name": "draperstudio/laravel-support",
    "type": "library",
    "description": "A collection of helper functions",
    "keywords": [
        "draperstudio",
        "laravel-support"
    ],
    "homepage": "https://github.com/DraperStudio/laravel-support",
    "license": "MIT",
    "authors": [{
        "name": "DraperStudio",
        "email": "hello@draperstudio.tech",
        "homepage": "http://draperstudio.tech",
        "role": "Developer"
    }],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "illuminate/support": "5.1.* || 5.2.*"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.8 || ^5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "files": [
            "src/Array.php",
            "src/Data.php",
            "src/Debug.php",
            "src/Http.php",
            "src/Laravel/Auth.php",
            "src/Laravel/Cartalyst/Sentinel.php",
            "src/Laravel/Console.php",
            "src/Laravel/Cryptography.php",
            "src/Laravel/Dispatcher.php",
            "src/Laravel/File.php",
            "src/Laravel/Filesystem.php",
            "src/Laravel/Form.php",
            "src/Laravel/Html.php",
            "src/Laravel/Image.php",
            "src/Laravel/Jwt.php",
            "src/Laravel/Log.php",
            "src/Laravel/Mail.php",
            "src/Laravel/Miscellaneous.php",
            "src/Laravel/Path.php",
            "src/Laravel/Validator.php",
            "src/Stringy.php",
            "src/TypeConversion.php",
            "src/Uuid.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DraperStudio\\Tests\\Support\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
