{
    "name": "faustbrian/helpers",
    "description": "A collection of helper functions",
    "keywords": ["helpers", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de"
    }],
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.3.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.2",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "files": [
            "src/Array.php",
            "src/Data.php",
            "src/Debug.php",
            "src/Http.php",
            "src/Laravel/Auth.php",
            "src/Laravel/Cartalyst/Sentinel.php",
            "src/Laravel/Console.php",
            "src/Laravel/Cryptography.php",
            "src/Laravel/Dispatcher.php",
            "src/Laravel/File.php",
            "src/Laravel/Filesystem.php",
            "src/Laravel/Form.php",
            "src/Laravel/Html.php",
            "src/Laravel/Image.php",
            "src/Laravel/Jwt.php",
            "src/Laravel/Log.php",
            "src/Laravel/Mail.php",
            "src/Laravel/Miscellaneous.php",
            "src/Laravel/Path.php",
            "src/Laravel/Validator.php",
            "src/Stringy.php",
            "src/TypeConversion.php",
            "src/Uuid.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Support\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
