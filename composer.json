{
    "name": "faustbrian/helpers",
    "description": "A collection of helper functions",
    "keywords": ["helpers", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de"
    }],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.3.* || 5.4.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.2",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "files": [
            "src/array_functions.php",
            "src/cartalyst_sentinel.php",
            "src/data_functions.php",
            "src/debug_functions.php",
            "src/http_functions.php",
            "src/laravel_auth_functions.php",
            "src/laravel_console_functions.php",
            "src/laravel_cryptography_functions.php",
            "src/laravel_dispatcher_functions.php",
            "src/laravel_file_functions.php",
            "src/laravel_filesystem_functions.php",
            "src/laravel_form_functions.php",
            "src/laravel_html_functions.php",
            "src/laravel_image_functions.php",
            "src/laravel_jwt_functions.php",
            "src/laravel_log_functions.php",
            "src/laravel_mail_functions.php",
            "src/laravel_miscellaneous_functions.php",
            "src/laravel_path_functions.php",
            "src/laravel_validator_functions.php",
            "src/string_functions.php",
            "src/type_conversion_functions.php",
            "src/uuid_functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Support\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
